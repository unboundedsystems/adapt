  "private": true,
  "engines": {
    "node": ">=10.17.0"
  },
  "devDependencies": {
    "@adpt/cli": "0.4.0-next.17",
    "@adpt/cloud": "0.4.0-next.17",
    "@adpt/testutils": "0.4.0-next.17",
    "@adpt/utils": "0.4.0-next.17",
    "@types/chai": "4.2.14",
    "@types/chai-as-promised": "7.1.3",
    "@types/dockerode": "3.2.1",
    "@types/fs-extra": "8.1.1",
    "@types/mocha": "8.0.4",
    "@types/node": "10.17.47",
    "@types/node-fetch": "2.5.7",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "dockerode": "3.2.1",
    "execa": "5.0.0",
    "fs-extra": "9.0.1",
    "mocha": "8.2.1",
    "mocha-slow-options": "0.0.2",
    "node-fetch": "2.6.1"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "clean": "rm -rf build dist generated temp",
    "coverage": "nyc mocha",
    "docs": "true",
    "lint": "tslint -e '**/*.json' --project .",
    "lint-and-fix": "tslint -e '**/*.json' --project . --fix",
    "prepack": "make build",
    "pretest": "make build",
    "test": "mocha"
  },
  "mocha": {
    "expose-gc": true,
    "recursive": true,
    "reporter": "mocha-slow-options",
    "require": [
      "test/init.js",
      "source-map-support/register"
    ],
    "spec": "dist/test",
    "timeout": 30000
  }
}
