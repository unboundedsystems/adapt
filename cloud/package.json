  "author": "Unbounded Systems",
  "contributors": [
    {
      "name": "Manish Vachharajani",
      "email": "manishv@unbounded.systems",
      "url": "https://twitter.com/mvachhar"
    },
    {
      "name": "Mark Terrel",
      "email": "mark@unbounded.systems",
      "url": "https://twitter.com/mterrel"
    }
  ],
  "repository": "unboundedsystems/adapt",
  "bugs": {
    "url": "https://gitlab.com/unboundedsystems/adapt/issues"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "adapt",
    "adaptjs",
    "adpt",
    "unbounded",
    "infrastructure-as-code",
    "devops",
    "deploy",
    "aws",
    "k8s",
    "kubernetes",
    "cloud",
    "deployment",
    "typescript",
    "nodejs",
    "react",
    "reactjs",
    "containers",
    "docker"
  ],
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "files": [
    "dist/src",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "make build && tsc -w -p tsconfig.json",
    "clean": "rm -rf build dist generated temp",
    "coverage": "nyc mocha",
    "docs": "cross-env ../scripts/make_docs.js cloud",
    "lint": "tslint -e '**/*.json' --project .",
    "lint-and-fix": "tslint -e '**/*.json' --project . --fix",
    "pretest": "make build",
    "test": "mocha"
  },
  "dependencies": {
    "@adpt/core": "0.4.0-next.9",
    "@adpt/utils": "0.4.0-next.9",
    "@types/node-fetch": "2.5.7",
    "aws-sdk": "2.797.0",
    "debug": "4.3.1",
    "diff": "5.0.0",
    "execa": "4.1.0",
    "fetchu": "2.0.1",
    "graphql": "14.7.0",
    "graphql-tools": "4.0.8",
    "graphql-type-json": "0.3.2",
    "js-yaml": "3.14.0",
    "json-stable-stringify": "1.0.1",
    "jsonschema": "1.4.0",
    "lodash": "4.17.20",
    "node-fetch": "2.6.1",
    "randomstring": "1.1.5",
    "shellwords-ts": "3.0.0",
    "swagger-client": "3.12.0",
    "tmp": "0.2.1",
    "ts-custom-error": "3.2.0",
    "tslib": "2.0.3",
    "type-ops": "3.0.3",
    "yup": "0.31.0"
  },
  "devDependencies": {
    "@adpt/testutils": "0.4.0-next.9",
    "@types/debug": "4.1.5",
    "@types/diff": "4.0.2",
    "@types/dockerode": "3.2.1",
    "@types/express": "4.17.8",
    "@types/fs-extra": "8.1.1",
    "@types/graphql-type-json": "0.3.2",
    "@types/js-yaml": "3.12.5",
    "@types/json-stable-stringify": "1.0.32",
    "@types/lodash": "4.14.165",
    "@types/mocha": "8.0.4",
    "@types/node": "10.17.46",
    "@types/randomstring": "1.1.6",
    "@types/tmp": "0.2.0",
    "@types/yup": "0.29.9",
    "aws-sdk-mock": "5.1.0",
    "dockerode": "3.2.1",
    "express": "4.17.1",
    "fs-extra": "9.0.1",
    "mocha": "8.2.1",
    "mocha-nock": "1.0.2",
    "mocha-slow-options": "0.0.2",
    "mocked-env": "1.3.2",
    "should": "13.2.3",
    "source-map-support": "0.5.19",
    "stream-buffers": "3.0.2"
  },
  "mocha": {
    "recursive": true,
    "reporter": "mocha-slow-options",
    "require": [
      "source-map-support/register",
      "@adpt/core/mock_adapt_context"
    ],
    "spec": "dist/test",
    "timeout": 30000
  }
}
